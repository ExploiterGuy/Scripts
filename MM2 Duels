OpenGui.Text = ">"
OpenGui.TextScaled = true
OpenGui.TextSize = 8
OpenGui.TextWrapped = true

OpenSwitch = false
OpenGui.MouseButton1Click:connect(function()
if OpenSwitch == false then
   OpenSwitch = true
   OpenGui.Text = "v"
   Frame1.Visible = true
   Frame2.Visible = true
   Frame3.Visible = true
else
   OpenSwitch = false
   OpenGui.Text = ">"
   Frame1.Visible = false
   Frame2.Visible = false
   Frame3.Visible = false
end
end)
